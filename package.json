{
  "name": "docdoc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "clean": "rm -rf node_modules && rm -rf out",
    "export": "next export",
    "start": "next start",
    "test": "jest  --verbose",
    "test:coverage": "jest --coverage --verbose",
    "test:watch": "jest --watch --verbose",
    "textlint": "npx textlint ./README.md",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "db:seed": "npx prisma db seed --schema ./src/app/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema ./src/app/prisma/schema.prisma",
    "db:drop": "npx prisma db query 'DROP DATABASE your_database_name' --schema ./src/app/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema ./src/app/prisma/schema.prisma",
    "db:reset": "npx prisma migrate reset --schema ./src/app/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema ./src/app/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-dist": "^3.30.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-ui-react": "^4.18.3",
    "@vercel/analytics": "^1.3.0",
    "@vercel/postgres": "^0.8.0",
    "autoprefixer": "10.4.16",
    "next": "^14.2.3",
    "prisma": "^5.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.17.12",
    "swagger-ui-express": "^5.0.0",
    "swagger-ui-react": "^5.17.10",
    "ts-node": "^10.9.2"
  },
  "prisma": {
    "seed": "ts-node src/app/prisma/seed.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@prisma/generator-helper": "^5.14.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "axios": "^1.7.1",
    "daisyui": "^4.0.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "prisma-erd-generator": "^1.11.2",
    "snyk": "^1.1291.0",
    "supertest": "^7.0.0",
    "tailwindcss": "3.4.0",
    "textlint": "^14.0.1",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "^1.0.1",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-number": "^1.0.0",
    "textlint-rule-preset-ja-spacing": "^2.4.3",
    "textlint-rule-preset-ja-technical-writing": "^8.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  }
}
