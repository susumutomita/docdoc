{
  "name": "docdoc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "clean": "rm -rf node_modules && rm -rf out",
    "export": "next export",
    "start": "next start",
    "test": "jest  --verbose",
    "test:coverage": "jest --coverage --verbose",
    "test:watch": "jest --watch --verbose",
    "textlint": "npx textlint ./README.md",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "db:drop": "npx prisma db query 'DROP DATABASE your_database_name' --schema ./src/app/pages/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema ./src/app/pages/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@vercel/analytics": "^1.2.1",
    "autoprefixer": "10.4.16",
    "next": "^14.2.3",
    "prisma": "^5.13.0",
    "react": "18.3.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.6.8",
    "daisyui": "^4.0.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "snyk": "^1.1291.0",
    "tailwindcss": "3.4.0",
    "textlint": "^14.0.1",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "^1.0.1",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-number": "^1.0.0",
    "textlint-rule-preset-ja-spacing": "^2.4.3",
    "textlint-rule-preset-ja-technical-writing": "^8.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  }
}
